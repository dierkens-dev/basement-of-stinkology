{
  "name": "basement-of-stinkology",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pulumi": "pulumi --cwd infrastructure/app",
    "build": "nuxt build",
    "predev": "pnpm codegen",
    "e2e": "cypress run",
    "cypress": "cypress open",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "typecheck": "nuxi typecheck",
    "precodegen": "rimraf src/features/movies/lib/moviedb",
    "codegen": "openapi -i \"https://developer.themoviedb.org/openapi/tmdb-api.json\" -o src/features/movies/lib/moviedb --useOptions --useUnionTypes --name MovieDbClient",
    "prepare": "husky install",
    "migrate": "prisma migrate dev"
  },
