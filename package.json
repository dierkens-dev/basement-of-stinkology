{
  "name": "basement-of-stinkology",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "main": "app.js",
  "scripts": {
    "pulumi": "pulumi --cwd infrastructure",
    "deploy": "yarn pulumi up",
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "run-p \"dev:*\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "@react-aria/utils": "^3.18.0",
    "@remix-run/express": "^1.18.1",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@remix-run/server-runtime": "^1.18.1",
    "@remix-validated-form/with-zod": "^2.0.6",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "firebase": "^9.23.0",
    "isbot": "^3.6.12",
    "morgan": "^1.10.0",
    "moviedb-promise": "^3.4.0",
    "prisma": "^4.16.2",
    "react": "^18.2.0",
    "react-aria": "^3.26.0",
    "react-date-picker": "^10.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-stately": "^3.24.0",
    "remix-auth": "^3.5.0",
    "remix-auth-form": "^1.3.0",
    "remix-validated-form": "^5.0.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@pulumi/docker": "^4.3.0",
    "@pulumi/gcp": "^6.59.0",
    "@pulumi/pulumi": "^3.74.0",
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "archiver": "^5.3.1",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.1.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,jsx}": "eslint --fix"
  }
}
