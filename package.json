    "folder-hash": "4.0.4",
    "husky": "^8.0.3",
    "moviedb-promise": "4.0.3",
    "next-auth": "4.21.1",
    "nodemailer": "6.9.7",
    "nodemailer-smtp-transport": "2.7.4",
    "nuxt": "3.19.2",
    "oh-vue-icons": "1.0.0-rc3",
